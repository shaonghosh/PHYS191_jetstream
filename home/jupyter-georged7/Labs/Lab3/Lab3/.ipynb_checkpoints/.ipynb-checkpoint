    "\n",
    "\n",
    "g = 9.8  # Acceleration due to gavity\n",
    "L = 1.2  # Rolling length of the plank. If you did the lab in campus this is 1.2 meter.\n",
    "\n",
    "# First angle measured using Physics Toolbox was about 2.26 degrees #\n",
    "theta_1 = np.deg2rad(2.26)\n",
    "\n",
    "# Second angle measured using Physics Toolbox was about 4.19 degrees #\n",
    "theta_2 = np.deg2rad(4.19)\n",
    "\n",
    "# Third angle measured using Physics Toolbox was about 5.94 degrees #\n",
    "theta_3 = np.deg2rad(5.94)\n",
    "\n",
    "# Fourth angle measured using Physics Toolbox was about 7.97 degrees #\n",
    "theta_4 = np.deg2rad(7.97)\n",
    "\n",
    "# Fifth angle measured using Physics Toolbox was about 9.54 degrees #\n",
    "theta_5 = np.deg2rad(9.54)\n"
   ]

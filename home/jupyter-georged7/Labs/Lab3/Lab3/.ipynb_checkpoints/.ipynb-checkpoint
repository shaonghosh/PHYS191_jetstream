   "source": [
    "### first calcualate the fractional errors ###\n",
    "Delta_approx = (D_approx - D_measured)/D_measured\n",
    "Delta_corrected = (D_corrected - D_measured)/D_measured\n",
    "\n",
    "### Now plot them with the measured values ###\n",
    "pl.rcParams.update({'font.size': 18})\n",
    "pl.figure(figsize=(12,10))\n",
    "pl.plot(D_measured, Delta_approx*100, 'ro-', label=\"Approximate result\")\n",
    "pl.plot(D_measured, Delta_corrected*100, 'bD-', label=\"Corrected result\")\n",
    "pl.xlabel('Measured value of the range (m)')\n",
    "pl.ylabel('Fractional percentage error in the estimation of the range')\n",
    "pl.legend()\n"
   ]
  },

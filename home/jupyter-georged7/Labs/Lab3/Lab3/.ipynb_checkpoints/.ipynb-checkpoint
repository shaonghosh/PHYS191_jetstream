    }
   ],
   "source": [
    "pl.rcParams.update({'font.size': 18})\n",
    "pl.figure(figsize=(12,10))\n",
    "\n",
    "pl.fill_between(D_measured, D_approx + uncertainty, D_approx - uncertainty, alpha=0.2, color='r')\n",
    "pl.plot(D_measured, D_approx, 'ro', label=\"Approximate measure of range\")\n",
    "pl.xlabel(\"Measured value of range (m)\")\n",
    "pl.ylabel(\"Inferred value of range (m)\")\n",
    "Min = np.min([D_measured, D_approx])\n",
    "Max = np.max([D_measured, D_approx])\n",
    "pl.plot([Min, Max], [Min, Max], 'k--', label=\"Line of consistency\")\n",
    "pl.legend()"
   ]

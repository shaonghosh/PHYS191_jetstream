   "source": [
    "pl.rcParams.update({'font.size': 18}) \n",
    "pl.figure(figsize=(12,10))\n",
    "pl.plot(delta_x_white, masses_white*g, 'k*-', label=\"White spring\", alpha=0.4)\n",
    "pl.plot(delta_x_white, fitting_func(delta_x_white, * popt_white), 'k--' )\n",
    "pl.plot(delta_x_blue, masses_blue*g, 'b*-', label=\"Blue spring\", alpha =0.4)\n",
    "pl.plot(delta_x_blue, fitting_func(delta_x_blue, * popt_blue), 'b--')\n",
    "pl.plot(delta_x_combined, masses_combined*g, 'r*-', label=\"blue & whit e spring combined\", alpha=0.4)\n",
    "pl.plot(delta_x_combined, fitting_func(delta_x_combined, * popt_combined), 'r--')\n",
    "\n",
    "pl.legend() \n",
    "pl.xlabel(\"$\\\\Delta x$\") \n",
    "pl.ylabel(\"$mg$\") \n",
    "pl.grid(1)"
   ]

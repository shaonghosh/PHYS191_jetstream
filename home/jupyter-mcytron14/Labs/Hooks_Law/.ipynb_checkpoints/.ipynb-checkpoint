   "source": [
    "from scipy.optimize import curve_fit"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fitting_func(x, a, b): \n",
    "    '''This function allows me to fit data linearly.\n",
    "The equation below is the equation of a straight line. '''\n",
    "    return a*x + b\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Slope = 42.913152271532645\n",
      "Intercept = -3.516614314407649\n"
     ]
    }
   ],
   "source": [
    "popt_white, pcov_white = curve_fit(fitting_func, delta_x_white, masses_white*g)\n",
    "\n",
    "popt_blue, pcov_blue = curve_fit(fitting_func, delta_x_blue, masses_blue*g)\n",
    "\n",
    "popt_combined, pcov_combined = curve_fit(fitting_func, delta_x_combined, masses_combined*g)\n",
    "\n",
    "# popt_white is just a tupple whose first element\n",
    "# is the slope of the fit and the second element is the intercept See below:\n",
    "print(\"Slope = {}\".format(popt_white[0]))\n",
    "print(\"Intercept = {}\".format(popt_white[1]))\n",
    "# pcov are the covariance matrices, where are way beyond the scope of this course. So, ignore that!"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",

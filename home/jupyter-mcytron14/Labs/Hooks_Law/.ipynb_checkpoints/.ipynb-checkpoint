   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Deviation from expected value = -12.48%\n"
     ]
    }
   ],
   "source": [
    "# Deviation from expected value:\n",
    "error = (k_combined_expected - k_combined)*100/k_combined_expected\n",
    "print(\"Deviation from expected value = {}%\".format(round(error, 2)))"
   ]

    "t_50_mean = np.mean(t_50)\n",
    "t_60_mean = np.mean(t_60)\n",
    "t_70_mean = np.mean(t_70)\n",
    "t_80_mean = np.mean(t_80)\n",
    "t_90_mean = np.mean(t_90)\n",
    "t_100_mean = np.mean(t_100)\n",
    "t_105_mean = np.mean(t_105)\n",
    "\n",
    "## Change this if you lengths are different ##\n",
    "x3 = np.array([20, 30, 40, 50, 60, 70, 80, 90, 100, 105])/100\n",
    "\n",
    "t3 = np.array([t_20_mean, t_30_mean, t_40_mean, t_50_mean, t_60_mean, \n",
    "              t_70_mean, t_80_mean, t_90_mean, t_100_mean, t_105_mean])\n",
    "\n",
    "v_avg_3 = x2/t3\n",
    "v3 = 2*v_avg_3\n",
    "v3_expect = 0.175*t3  # USE YOUR VALUE OF g.sin(theta) = 9.8*sin(theta2)FOR THIS ANGLE.\n",
    "\n"
   ]
  },
